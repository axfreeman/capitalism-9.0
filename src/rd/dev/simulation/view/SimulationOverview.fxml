<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1300.0"
	xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="rd.dev.simulation.view.ViewManager">
	<children>
		<VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
			AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
			<children>
				<HBox fx:id="displayControlsBox" maxWidth="1.7976931348623157E308"
					prefHeight="50.0">
					<children>
						<ButtonBar fx:id="leftButtonBar">
							<buttons>
								<Button fx:id="dataDumpButton" mnemonicParsing="false"
									text="Save">
									<font>
										<Font size="10.0" />
									</font>
								</Button>
								<Button fx:id="loadButton" alignment="CENTER"
									mnemonicParsing="false" text="Load">
									<font>
										<Font size="10.0" />
									</font>
								</Button>
								<Button fx:id="restartButton" mnemonicParsing="false"
									text="Restart">
									<font>
										<Font size="10.0" />
									</font>
								</Button>
							</buttons>
						</ButtonBar>
						<Pane prefHeight="200.0" prefWidth="50.0" HBox.hgrow="ALWAYS">
							<children>
							</children>
						</Pane>
						<ButtonBar fx:id="rightButtonBar" nodeOrientation="LEFT_TO_RIGHT">

							<buttons>
								<Button fx:id="colourHintButton" layoutX="560.0" layoutY="13.0"
									mnemonicParsing="false" text="Hints" ButtonBar.buttonData="RIGHT" />
								<Button fx:id="toggleValuesExpressionButton"
									mnemonicParsing="false" text="#values" ButtonBar.buttonData="RIGHT" />
								<Button fx:id="togglePricesExpressionButton"
									mnemonicParsing="false" text="#prices" ButtonBar.buttonData="RIGHT" />
								<Button fx:id="toggleDecimalButton" mnemonicParsing="false"
									text="..&gt;&gt;" ButtonBar.buttonData="RIGHT">
									<font>
										<Font size="10.0" />
									</font>
								</Button>
								<Button fx:id="graphicsToggle" mnemonicParsing="false"
									text="Graphics" ButtonBar.buttonData="RIGHT">
									<font>
										<Font size="10.0" />
									</font>
								</Button>
								<Button fx:id="logButton" alignment="CENTER"
									mnemonicParsing="false" text="Log" ButtonBar.buttonData="RIGHT">
									<font>
										<Font size="10.0" />
									</font>
								</Button>

							</buttons>
							<opaqueInsets>
								<Insets bottom="5.0" />
							</opaqueInsets>
						</ButtonBar>
					</children>
				</HBox>
				<HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
					VBox.vgrow="ALWAYS">
					<children>
						<VBox fx:id="controlsVBox" maxWidth="-Infinity" minWidth="-Infinity"
							prefHeight="700.0" prefWidth="150.0">
							<children>
								<GridPane VBox.vgrow="NEVER">
									<columnConstraints>
										<ColumnConstraints hgrow="NEVER" minWidth="10.0"
											prefWidth="100.0" />
										<ColumnConstraints hgrow="SOMETIMES"
											minWidth="10.0" prefWidth="100.0" />
									</columnConstraints>
									<rowConstraints>
										<RowConstraints prefHeight="30.0" vgrow="SOMETIMES" />
									</rowConstraints>
									<children>
										<Label fx:id="timeStampCursorLabel" contentDisplay="RIGHT"
											prefHeight="30.0" text="Label" textFill="#0b26bc" />
										<Label fx:id="projectCursorLabel" contentDisplay="RIGHT"
											text="Label" textFill="#0b26bc" GridPane.columnIndex="1" />
									</children>
									<VBox.margin>
										<Insets left="5.0" right="5.0" />
									</VBox.margin>
								</GridPane>
								<Separator prefWidth="200.0" />
								<Label text="Show Stocks as..." />
								<RadioButton fx:id="showPricesButton" minHeight="-Infinity"
									mnemonicParsing="false" prefHeight="17.0" prefWidth="81.0"
									text="Prices">
									<padding>
										<Insets right="2.0" />
									</padding>
								</RadioButton>
								<RadioButton fx:id="showQuantitiesButton" minHeight="-Infinity"
									mnemonicParsing="false" prefHeight="17.0" prefWidth="104.0"
									text="Quantities" />
								<RadioButton fx:id="showValuesButton" minHeight="-Infinity"
									mnemonicParsing="false" prefHeight="17.0" prefWidth="84.0"
									text="Values">
									<padding>
										<Insets right="2.0" />
									</padding>
								</RadioButton>
								<Separator prefWidth="200.0" />
								<Label text="Compare with..." />
								<RadioButton fx:id="previousSelectorButton"
									mnemonicParsing="false" text="Previous" />
								<RadioButton fx:id="startSelectorButton"
									mnemonicParsing="false" text="Start" />
								<RadioButton fx:id="endSelectorButton"
									mnemonicParsing="false" text="End" />
								<RadioButton fx:id="customSelectorButton"
									mnemonicParsing="false" text="Custom" />
								<Separator prefWidth="200.0" />
								<Label text="Simulation...">
									<VBox.margin>
										<Insets top="10.0" />
									</VBox.margin>
								</Label>
							</children>
						</VBox>
						<VBox fx:id="simulationResultsPane" maxHeight="1.7976931348623157E308"
							maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="836.0"
							HBox.hgrow="ALWAYS" />
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>
